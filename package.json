    "pull-reconnect": "^0.0.3",
    "pull-scroll": "^1.0.3",
    "pull-stream": "^3.4.5",
    "scuttlebot": "^8.7.2",
    "setimmediate": "^1.0.5",
    "simple-mime": "^0.1.0",
    "split-buffer": "^1.0.0",
    "ssb-avatar": "^0.2.0",
    "ssb-blobs": "^0.1.7",

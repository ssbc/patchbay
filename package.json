    "ssb-ws": "^1.0.1",
    "suggest-box": "^2.2.1",
    "text-node-searcher": "^1.1.0",
    "visualize-buffer": "0.0.1"
  },
  "devDependencies": {
    "browselectrify": "^1.0.1",
    "electro": "^2.0.3",
    "electron": "^1.4.10",
    "indexhtmlify": "^1.3.1",
    "tape": "^4.6.3",
    "tape-run": "^2.1.5"
  },
  "browserify": {
    "transform": [
      "bulkify",
      "brfs",
      "es2040"
